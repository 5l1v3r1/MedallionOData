<?xml version="1.0"?>
<package >
  <metadata>
    <id>MedallionOData</id>
    <version>1.5.0</version>
    <title>Medallion OData</title>
    <authors>Michael Adelson</authors>
    <licenseUrl>https://raw.githubusercontent.com/madelson/MedallionOData/master/License.txt</licenseUrl>
    <projectUrl>https://github.com/madelson/MedallionOData</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>
      A lightweight, zero-setup .NET library for creating and querying OData and OData-like services
    </summary>
    <description>
      MedallionOData is a lightweight, zero-setup .NET library for creating and querying OData and OData-like services.
    </description>
    <releaseNotes>
      Version 1.5.0 adds support for .NET Core via .NET Standard 1.5. Dynamic ODataEntity queries are not supported in the .NET Standard build
    </releaseNotes>
    <copyright>Copyright 2017</copyright>
    <tags>odata linq ef entity framework ado.net sql mvc webapi</tags>
    <dependencies>
	  <group targetFramework="net45">
	    <dependency id="Newtonsoft.Json" version="4.5.11" />
	  </group>
      <group targetFramework="netstandard1.5">
        <dependency id="NETStandard.Library" version="1.6.0"/>
		<dependency id="System.Data.Common" version="4.3.0"/>
        <dependency id="System.Data.SqlClient" version="4.3.0"/>
		<dependency id="System.Collections.Specialized" version="4.3.0"/>
		<dependency id="System.Linq.Queryable" version="4.3.0"/>
		<dependency id="System.Reflection" version="4.3.0"/>
		<dependency id="System.Reflection.TypeExtensions" version="4.3.0"/>
		<dependency id="Newtonsoft.Json" version="9.0.1" />
      </group>
    </dependencies>
  </metadata>
  <files>
	<file src="MedallionOData\bin\Debug\MedallionOData.dll" target="lib\net45"/>
    <file src="MedallionOData\bin\Debug\MedallionOData.XML" target="lib\net45"/>
